{
    "kind": "template",
    "name": "dashui:ecpListInspector",
    "target": "labsol:ucs_domain",
    "view": "default",
    "element": {
        "instanceOf": "elements",
        "elements": [
            {
                "instanceOf": "focusedEntities",
                "element": {
                    "instanceOf": {
                        "name": "nameWidget"
                    },
                    "path": [
                        "attributes(labsol.host.name)",
                        "id"
                    ]
                },
                "mode": "SINGLE"
            },
            {
                "instanceOf": "focusedEntities",
                "element": {
                    "instanceOf": {
                        "name": "labsol:ucs_domainInspectorWidget"
                    }
                },
                "mode": "SINGLE"
            }
        ]
    }
}
